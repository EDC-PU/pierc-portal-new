
rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Only authenticated users can access files.
    // This is a basic rule. For production, you would want to restrict access further.
    // For example, only the uploader or specific users should be able to read/write certain paths.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
